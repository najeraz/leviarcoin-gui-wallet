{
    "name": "leviarcoin-gui-wallet",
    "productName": "leviarcoin-gui-wallet",
    "version": "0.1.2",
    "description": "Send and Receive LeviarCoin",
    "main": "src/main.js",
    "postinstall": "electron-builder install-app-deps",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish"
    },
    "keywords": [],
    "author": "Leviar",
    "license": "MIT",
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {
                "packageManager": "npm",
                "icon": "src/resources/images/icon.ico"
            },
            "electronWinstallerConfig": {
                "name": "leviarcoin_gui_wallet"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "",
                "name": ""
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "leviarcoinelectronwallet"
            }
        }
    },
    "dependencies": {
        "crypto": "^1.0.1",
        "electron-compile": "^6.4.2",
        "electron-modal": "^1.0.0",
        "electron-store": "^1.3.0",
        "electron-tooltip": "^1.1.4"
    },
    "devDependencies": {
        "electron-forge": "4.1.3",
        "electron-prebuilt-compile": "1.8.1"
    }
}
